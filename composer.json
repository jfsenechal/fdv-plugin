{
  "name": "acmarche/fdv-plugin",
  "authors": [
    {
      "name": "jfsenechal",
      "email": "jfsenechal@gmail.com"
    }
  ],
  "version": "1.0.0",
  "require": {
    "php": ">=8.3",
    "symfony/dotenv": "^7.3",
    "symfony/http-client": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "Fdv\\Plugin\\": "wp-content/plugins/fdv/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.3.*",
      "docker": false
    }
  },
  "require-dev": {
    "symfony/error-handler": "7.3.*"
  }
}
